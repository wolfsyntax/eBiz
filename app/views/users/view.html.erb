<h1>Account Details</h1>
<div>
	
	<p>Username: &emsp;<%= @user.username %><br/></p>
	<p>Password: &emsp;<%= @user.password_digest %><br/></p>
	<p>Account Type: &emsp;<%= @user.account_type %><br/></p>
	
	<% if session[:username] != nil %>
		<%= link_to 'Login', '/dashboard' %>
	<% else %>
		<%= link_to 'Login', '/login.do' %>
	<% end %>
link_to
</div>